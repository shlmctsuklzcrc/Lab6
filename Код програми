#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int x1,x2,y1,y2;
    printf("Введіть координати початку вектора(x1,y1)\n");
    scanf("%d%d",&x1,&y1);
    printf("Введіть кординати кінця вектора(x2,y2)\n");
    scanf("%d%d",&x2,&y2);
    float length = sqrt(pow(x1-x2,2)+ pow(y1-y2,2));
    printf("Дловжина заданого вектора %f",length);

    return 0;
}
